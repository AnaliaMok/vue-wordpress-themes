{
	"compilerOptions": {
		"target": "es5",
		"strict": true,
		"module": "es2015",
		"moduleResolution": "node",
		"experimentalDecorators": true,
		"jsx": "preserve",
		"jsxFactory": "h",
	},
	"exclude": ["node_modules"],
	"include": [
		"assets/vue/**/*.ts",
		"assets/vue/**/*.tsx"
	],
	"files": [
		"assets/vue/jsx.d.ts"
	]
}